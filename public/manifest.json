{
  "manifest_version": 2,
  "name": "Tabs Manger",
  "description": "list opened all tabs, choose what I need",
  "version": "1.0",
  "icons": {
    "16": "img/icon-16.png",
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon-19.png",
      "38": "img/icon-38.png"
    },
    "defaule_title": "Tabs"
  },
  "permissions": ["tabs", "background", "<all_urls>", "storage"],
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "persistent": false,
    "page": "background.html"
  },
  "commands": {
    "_execute_browser_action": {
      "suggested_key": {
        "default": "Alt+Shift+T"
      }
    }
  }
}
