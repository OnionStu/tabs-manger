{
  "manifest_version": 2,
  "name": "Tabs Manger",
  "description": "list opened all tabs, choose what I need",
  "version": "1.1",
  "icons": {
    "16": "img/icon-16.png",
    "128": "img/icon-128.png"
  },
  "browser_action": {
    "default_icon": {
      "19": "img/icon-19.png",
      "38": "img/icon-38.png"
    },
    "default_title": "Tabs"
  },
  "permissions": ["tabs", "<all_urls>", "storage"],
  "background": {
    "scripts": ["static/js/background.js"]
  },
  "commands": {
    "toggle-feature": {
      "suggested_key": {
        "default": "Ctrl+Shift+U"
      },
      "description": "Send a 'toggle-feature' event to the extension"
    }
  }
}
